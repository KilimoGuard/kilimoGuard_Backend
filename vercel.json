{
    "version": 2,
    "builds": [
      {
        "src": "kilimo_project/wsgi.py",
        "use": "@vercel/python",
        "config": { "maxLambdaSize": "15mb" }
      }
    ],
    "routes": [
      {
        "src": "/static/(.*)",
        "dest": "/static/$1"
      },
      {
        "src": "/(.*)",
        "dest": "kilimo_project/wsgi.py"
      }
    ]
  }
  